{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "ssm:PutParameter",
            "ssm:GetParameter"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy",
    "event_sources": [
      {
        "resource_type": "sqs_trigger",
        "target_queue": "async_queue",
        "batch_size": 10
      }
    ]
  },
  "async_queue": {
    "resource_type": "sqs_queue",
    "fifo_queue": false,
    "region": "eu-central-1",
    "visibility_timeout": 60,
    "delay_seconds": 10,
    "maximum_message_size": 1024,
    "message_retention_period": 120,
    "receive_message_wait_time_seconds": 20,
    "policy": {},
    "redrive_policy": {},
    "content_based_deduplication": false
  }
}
